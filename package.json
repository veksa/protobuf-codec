{
  "name": "@veksa/protobuf-codec",
  "version": "0.0.1",
  "description": "Protocol Buffers codec implementation for @veksa/transport messaging layer",
  "homepage": "https://github.com/veksa/protobuf-codec",
  "packageManager": "yarn@4.9.2",
  "author": {
    "name": "Aleksandr Khizhnyi",
    "email": "mendler88@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:veksa/protobuf-codec.git"
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "protobuf",
    "protocol-buffers",
    "codec",
    "serialization",
    "transport",
    "messaging",
    "typescript",
    "encoding",
    "decoding"
  ],
  "scripts": {
    "clean": "rimraf lib es dist",
    "build": "yarn clean && tsup",
    "compile": "tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@veksa/transport": "^0.0.3",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "protobufjs": "^7.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
